<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#502E2F47"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"/>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/white">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_48">
            <TextView
                style="@style/common_txt_style_14_666"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:text="@string/receive_warn_txt_03"/>
            <ImageView
                android:id="@+id/map_transfer_dialog_img_close"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:padding="@dimen/dp_10"
                android:src="@mipmap/icon_pop_transfer_close"/>
        </RelativeLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:padding="@dimen/dp_10">
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_25">
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center|bottom">
                    <TextView
                        android:id="@+id/map_transfer_dialog_txt_money"
                        style="@style/common_txt_style_15_333"
                        android:textColor="@color/color_2E2F47"/>
                    <TextView
                        style="@style/common_txt_style_14_666"
                        android:layout_centerHorizontal="true"
                        android:layout_marginLeft="@dimen/dp_8"
                        android:text="@string/wallet_manager_txt_money_unit_01"/>
                </LinearLayout>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <TextView
                    style="@style/common_txt_style_14_666"
                    android:text="@string/receive_warn_txt_04"/>
                <TextView
                    style="@style/common_txt_style_14_666"
                    android:layout_alignParentRight="true"
                    android:text="@string/activity_main_map_txt_05"/>
            </RelativeLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_25">
                <TextView
                    style="@style/common_txt_style_14_666"
                    android:text="@string/activity_main_map_txt_10"/>
                <TextView
                    android:id="@+id/map_transfer_dialog_txt_to_address"
                    style="@style/common_txt_style_14_666"
                    android:textColor="@color/color_66688F"
                    android:layout_marginLeft="30dp"
                    android:gravity="right"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_25">
                <TextView
                    style="@style/common_txt_style_14_666"
                    android:layout_centerVertical="true"
                    android:text="@string/activity_main_map_txt_08"/>
                <TextView
                    android:id="@+id/map_transfer_dialog_txt_from_address"
                    style="@style/common_txt_style_14_666"
                    android:textColor="@color/color_66688F"
                    android:layout_marginLeft="30dp"
                    android:gravity="right"/>
            </LinearLayout>
            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_25">
                <TextView
                    style="@style/common_txt_style_14_666"
                    android:text="@string/transfer_activity_txt_10"/>
                <TextView
                    android:id="@+id/map_transfer_dialog_txt_fee"
                    style="@style/common_txt_style_14_666"
                    android:layout_alignParentRight="true"
                    android:layout_marginLeft="@dimen/dp_18"/>
            </RelativeLayout>
            <Button
                android:id="@+id/map_transfer_dialog_btn_submit"
                style="@style/common_btn_style_defult"
                android:layout_marginTop="@dimen/dp_36"
                android:layout_marginBottom="@dimen/dp_15"
                android:textColor="@color/white"
                android:textAllCaps="false"
                android:background="@drawable/draw_btn_defult_bg_03"
                android:text="@string/dailog_psd_btn_ok" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>