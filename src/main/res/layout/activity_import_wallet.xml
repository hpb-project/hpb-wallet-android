<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:title="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@color/white">

    <include layout="@layout/common_toolbar" />

    <com.zhaoxi.Open_source_Android.common.view.tabview.TabPageIndicator
        android:id="@+id/import_wallet_activity_tabviews"
        android:layout_width="match_parent"
        android:layout_height="43dp" />
    <com.zhaoxi.Open_source_Android.common.view.tabview.UnderlinePageIndicatorEx
        android:id="@+id/import_wallet_activity_tabviews_line"
        style="@style/CustomStyledIndicators"
        android:layout_width="match_parent"
        android:layout_height="2dp" />
    <View style="@style/common_line_horizontal"/>
    <android.support.v4.view.ViewPager
        android:id="@+id/import_wallet_activity_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>