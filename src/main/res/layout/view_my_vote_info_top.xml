<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_margin="@dimen/dp_18"
    android:background="@drawable/bg_vote_myvote_layout">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <TextView
            style="@style/common_txt_style_14_666"
            android:textColor="@color/white"
            android:layout_centerVertical="true"
            android:text="@string/activity_vote_txt_recode_01"/>
        <TextView
            android:id="@+id/view_my_vote_top_01"
            style="@style/common_txt_style_14_666"
            android:textColor="@color/white"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true" />
    </RelativeLayout>
    <View android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/color_2E2F47"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="45dp">
        <TextView
            style="@style/common_txt_style_14_666"
            android:textColor="@color/white"
            android:layout_centerVertical="true"
            android:text="@string/activity_vote_txt_recode_02"/>
        <TextView
            android:id="@+id/view_my_vote_top_02"
            style="@style/common_txt_style_14_666"
            android:textColor="@color/white"
            android:layout_centerVertical="true"
            android:layout_alignParentRight="true" />
    </RelativeLayout>
</LinearLayout>