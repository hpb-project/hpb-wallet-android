<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    android:background="#6E6E6E">
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@mipmap/icon_vote_wait_top"/>
    <TextView
        style="@style/common_txt_style_14_666"
        android:layout_margin="@dimen/dp_25"
        android:gravity="center"
        android:text="@string/activity_vote_txt_26"/>
    <Button
        android:id="@+id/activity_vote_Wait_back"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_40"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/dp_18"
        android:background="@mipmap/icon_vote_wait_btn_bg"
        android:text="@string/activity_vote_txt_27"/>
</LinearLayout>
