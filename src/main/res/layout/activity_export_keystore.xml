<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/white">
    <include layout="@layout/common_toolbar" />

    <com.zhaoxi.Open_source_Android.common.view.tabview.TabPageIndicator
        android:id="@+id/export_keystore_activity_tabviews"
        android:layout_width="match_parent"
        android:layout_height="43dp" />
    <com.zhaoxi.Open_source_Android.common.view.tabview.UnderlinePageIndicatorEx
        android:id="@+id/export_keystore_activity_tabviews_line"
        style="@style/CustomStyledIndicators"
        android:layout_width="match_parent"
        android:layout_height="2.5dp" />

    <View style="@style/common_line_horizontal" />

    <android.support.v4.view.ViewPager
        android:id="@+id/export_keystore_activity_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>