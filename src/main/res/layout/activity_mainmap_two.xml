<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/base_theme_color"
    android:orientation="vertical">
    <include layout="@layout/common_toolbar" />

    <com.zhaoxi.Open_source_Android.common.view.refresh.MyPtrFrameLayout
        android:id="@+id/activity_main_map_two_ptrframelayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/activity_main_map_two_recyclerView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@id/wallet_manager_bottom"
            android:layout_below="@id/common_toolbar"
            android:layout_marginTop="@dimen/dp_12"
            android:visibility="visible" />
    </com.zhaoxi.Open_source_Android.common.view.refresh.MyPtrFrameLayout>
</LinearLayout>
